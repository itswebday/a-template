{
  "name": "a-template",
  "version": "1.0.0",
  "description": "Template for It's Webday A-Projects",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build --webpack",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@payloadcms/db-vercel-postgres": "3.65.0",
    "@payloadcms/live-preview-react": "3.65.0",
    "@payloadcms/next": "3.65.0",
    "@payloadcms/plugin-seo": "3.65.0",
    "@payloadcms/richtext-lexical": "3.65.0",
    "@payloadcms/storage-vercel-blob": "3.65.0",
    "@payloadcms/ui": "3.65.0",
    "@reduxjs/toolkit": "^2.11.0",
    "@tailwindcss/postcss": "^4.1.17",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "motion": "^12.23.24",
    "next": "^16.0.8",
    "next-intl": "^4.5.5",
    "next-sitemap": "^4.2.3",
    "payload": "3.65.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "sharp": "0.34.2",
    "swiper": "^12.0.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9.16.0",
    "eslint-config-next": "^16.0.8",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
